{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "electric_vehicle_telemetry",
    "title": "Electric Vehicle Telemetry",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "stateOfCharge": {
            "type": "number"
        },
        "estimatedChargingTime": {
            "type": "number"
        },
        "voltage": {
            "type": "number"
        },
        "current": {
            "type": "number"
        },
        "activePower": {
            "type": "number"
        }
    },
    "required": [
        "stateOfCharge",
        "estimatedChargingTime",
        "voltage",
        "current",
        "activePower"
    ]
}