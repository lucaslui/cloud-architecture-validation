{
    "deviceId": "AC-HOME-${HOME_NUMBER}",
    "deviceType": "HVAC",
    "eventType": "Telemetry",
    "schemaVersion": "1.0",
    "timestamp": "${TIMESTAMP}",
    "payload": {
        "indoorTempC": "${__Random(18,30)}",
        "indoorHumidityPct": "${__Random(25,80)}",
        "outdoorTempC": "${__Random(10,40)}",
        "setpointC": "${__Random(20,26)}",
        "fanRpm": "${__Random(400,1400)}",
        "activePowerW": "${__Random(200,2500)}",
        "mode": "${__chooseRandom(Cool,Heat,Fan,Auto)}"
    }
}