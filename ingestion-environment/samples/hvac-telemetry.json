{
    "deviceId": "AC-HOME-${HOME_NUMBER}",
    "deviceType": "HVAC",
    "eventType": "Telemetry",
    "schemaVersion": "1.0",
    "timestamp": "${__time(yyyy-MM-dd'T'HH:mm:ss'Z')}",
    "payload": {
        "indoorTempC": "${__Random(18,30)}",
        "indoorHumidityPct": "${__Random(25,80)}",
        "outdoorTempC": "${__Random(10,40)}",
        "setpointC": "${__Random(20,26)}",
        "fanRpm": "${__Random(400,1400)}",
        "activePowerW": "${__Random(200,2500)}",
        "mode": "${__chooseRandom(Cool|Heat|Fan|Auto)}"
    },
    "metadata": {
        "regionId": "SP-Campinas-${__Random(1,5)}",
        "deviceLocation": "LivingRoom",
        "deviceCategory": "HVAC",
        "deviceModel": "DaikinX${__Random(100,999)}",
        "endUserId": "user-${__Random(1,9999)}"
    }
}