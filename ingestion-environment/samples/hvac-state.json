{
    "deviceId": "AC-HOME-${HOME_NUMBER}",
    "deviceType": "HVAC",
    "eventType": "State",
    "schemaVersion": "1.0",
    "timestamp": "${__time(yyyy-MM-dd'T'HH:mm:ss'Z')}",
    "payload": {
        "powerState": "${__chooseRandom(On,Off)}",
        "operatingMode": "${__chooseRandom(Cooling,Heating,Ventilating)}",
        "setpointTemperature": "${__Random(18,26)}"
    }
}