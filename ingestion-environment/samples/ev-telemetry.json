{
    "deviceId": "EV-${__RandomString(6,abcdef0123456789)}",
    "deviceType": "ElectricVehicle",
    "eventType": "Telemetry",
    "schemaVersion": "1.0",
    "timestamp": "${__time(yyyy-MM-dd'T'HH:mm:ss'Z')}",
    "payload": {
        "stateOfCharge": "${__Random(10,100)}",
        "estimatedChargingTime": "${__Random(600,14400)}",
        "voltage": "${__Random(210,230)}",
        "current": "${__Random(5,32)}",
        "activePower": "${__Random(1000,7000)}"
    },
    "metadata": {
        "regionId": "SP-Campinas-${__Random(1,5)}",
        "deviceLocation": "Garage",
        "deviceCategory": "EV",
        "deviceModel": "TeslaModel${__Random(1,3)}",
        "endUserId": "user-${__Random(1,9999)}",
        "contractType": "${__chooseRandom(TimeOfUse|Conventional)}",
        "programs": [
            "DemandResponse",
            "RenewableIntegration"
        ]
    }
}