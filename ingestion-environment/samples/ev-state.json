{
    "deviceId": "EV-${__RandomString(6,abcdef0123456789)}",
    "deviceType": "ElectricVehicle",
    "eventType": "State",
    "schemaVersion": "1.0",
    "timestamp": "${__time(yyyy-MM-dd'T'HH:mm:ss'Z')}",
    "payload": {
        "pluggedIn": "${__chooseRandom(True,False)}",
        "chargingStatus": "${__chooseRandom(Charging,NotCharging,Completed,Fault)}",
        "connectorType": "${__chooseRandom(Type2,CCS,CHAdeMO)}",
        "lockState": "${__chooseRandom(Locked,Unlocked)}"
    },
    "metadata": {
        "regionId": "SP-Campinas-${__Random(1,5)}",
        "deviceLocation": "Garage",
        "deviceCategory": "EV",
        "deviceModel": "TeslaModel${__Random(1,3)}",
        "endUserId": "user-${__Random(1,9999)}"
    }
}