{
    "deviceId": "RES-HOME-${HOME_NUMBER}",
    "deviceType": "RES",
    "eventType": "Telemetry",
    "schemaVersion": "1.0",
    "timestamp": "${__time(yyyy-MM-dd'T'HH:mm:ss'Z')}",
    "payload": {
        "generatedPowerW": "${__Random(0,7000)}",
        "inverterVoltage": "${__Random(210,240)}",
        "inverterCurrent": "${__Random(0,40)}",
        "frequencyHz": "${__Random(59,61)}",
        "powerFactor": "${__Random(90,100)}",
        "solarIrradianceWm2": "${__Random(0,1100)}"
    }
}