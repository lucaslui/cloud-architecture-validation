{
    "deviceId": "ES-HOME-${HOME_NUMBER}",
    "deviceType": "EnvironmentalSensor",
    "eventType": "State",
    "schemaVersion": "1.0",
    "timestamp": "${TIMESTAMP}",
    "payload": {
        "sensorStatus": "${__chooseRandom(Active,Inactive,Faulty)}",
        "batteryLevelPct": "${__Random(10,100)}",
        "occupancy": "${__chooseRandom(Occupied,Vacant)}"
    }
}