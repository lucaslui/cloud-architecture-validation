{
    "deviceId": "BAT-${__RandomString(6,abcdef0123456789)}",
    "deviceType": "BatteryBank",
    "eventType": "State",
    "schemaVersion": "1.0",
    "timestamp": "${__time(yyyy-MM-dd'T'HH:mm:ss'Z')}",
    "payload": {
        "connectionState": "${__chooseRandom(Connected|Disconnected)}",
        "chargeState": "${__chooseRandom(Charging|Discharging|Idle)}",
        "health": "${__chooseRandom(Good|Degraded|Critical)}"
    },
    "metadata": {
        "regionId": "SP-Campinas-${__Random(1,5)}",
        "deviceLocation": "UtilityRoom",
        "deviceCategory": "Battery",
        "deviceModel": "ESS-${__Random(100,999)}",
        "endUserId": "user-${__Random(1,9999)}"
    }
}