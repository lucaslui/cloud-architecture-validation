{
    "deviceId": "WM-HOME-${HOME_NUMBER}",
    "deviceType": "Appliance",
    "eventType": "Telemetry",
    "schemaVersion": "1.0",
    "timestamp": "${__time(yyyy-MM-dd'T'HH:mm:ss'Z')}",
    "payload": {
        "voltage": "${__Random(210,230)}",
        "current": "${__Random(0,12)}",
        "activePowerW": "${__Random(0,2500)}",
        "reactivePowerVar": "${__Random(0,1250)}",
        "apparentPowerVA": "${__Random(0,2800)}",
        "powerFactorPct": "${__Random(70,100)}",
        "frequencyHz": "${__Random(59,61)}"
    },
    "metadata": {
        "regionId": "SP-Campinas-${__Random(1,5)}",
        "deviceLocation": "Laundry",
        "deviceCategory": "WashingMachine",
        "deviceModel": "WM-${__Random(100,999)}",
        "endUserId": "user-${__Random(1,9999)}"
    }
}