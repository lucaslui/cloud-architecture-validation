{
    "deviceId": "TV-HOME-${HOME_NUMBER}",
    "deviceType": "Appliance",
    "eventType": "Telemetry",
    "schemaVersion": "1.0",
    "timestamp": "${__time(yyyy-MM-dd'T'HH:mm:ss'Z')}",
    "payload": {
        "voltage": "${__Random(210,230)}",
        "current": "${__Random(0,2)}",
        "activePowerW": "${__Random(0,200)}",
        "reactivePowerVar": "${__Random(0,100)}",
        "apparentPowerVA": "${__Random(0,220)}",
        "powerFactorPct": "${__Random(70,100)}",
        "frequencyHz": "${__Random(59,61)}"
    },
    "metadata": {
        "regionId": "SP-Campinas-${__Random(1,5)}",
        "deviceLocation": "LivingRoom",
        "deviceCategory": "Television",
        "deviceModel": "TV-${__Random(100,999)}",
        "endUserId": "user-${__Random(1,9999)}"
    }
}