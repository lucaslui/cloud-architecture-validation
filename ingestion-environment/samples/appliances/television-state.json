{
    "deviceId": "TV-${__RandomString(6,abcdef0123456789)}",
    "deviceType": "Appliance",
    "eventType": "State",
    "schemaVersion": "1.0",
    "timestamp": "${__time(yyyy-MM-dd'T'HH:mm:ss'Z')}",
    "payload": {
        "powerState": "${__chooseRandom(On|Off|Standby)}",
        "inputSource": "${__chooseRandom(HDMI1|HDMI2|Tuner|App)}",
