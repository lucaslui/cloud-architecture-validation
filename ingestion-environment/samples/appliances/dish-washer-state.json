    "metadata": {
        "regionId": "SP-Campinas-${__Random(1,5)}",
        "deviceLocation": "Kitchen",
        "deviceCategory": "Dishwasher",
        "deviceModel": "DW-${__Random(100,999)}",
        "endUserId": "user-${__Random(1,9999)}"
    }
}