{
    "deviceId": "DW-${__RandomString(6,abcdef0123456789)}",
    "deviceType": "Appliance",
    "eventType": "State",
    "schemaVersion": "1.0",
    "timestamp": "${__time(yyyy-MM-dd'T'HH:mm:ss'Z')}",
    "payload": {
        "operationStatus": "${__chooseRandom(On|Off)}",
        "cyclePhase": "${__chooseRandom(Idle|Prewash|Wash|Rinse|Dry|Completed)}",
