{
    "deviceId": "RF-HOME-${HOME_NUMBER}",
    "deviceType": "Appliance",
    "eventType": "Telemetry",
    "schemaVersion": "1.0",
    "timestamp": "${TIMESTAMP}",
    "payload": {
        "voltage": "${__Random(210,230)}",
        "current": "${__Random(0,4)}",
        "activePowerW": "${__Random(0,300)}",
        "reactivePowerVar": "${__Random(0,300)}",
        "apparentPowerVA": "${__Random(0,350)}",
        "powerFactorPct": "${__Random(70,100)}",
        "frequencyHz": "${__Random(59,61)}"
    }
}