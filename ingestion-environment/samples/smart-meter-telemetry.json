{
    "deviceId": "SM-HOME-${HOME_NUMBER}",
    "deviceType": "SmartMeter",
    "eventType": "Telemetry",
    "schemaVersion": "1.0",
    "timestamp": "${__time(yyyy-MM-dd'T'HH:mm:ss'Z')}",
    "payload": {
        "energyConsumption": "${__Random(0,500)}",
        "energyGeneration": "${__Random(0,300)}",
        "voltage": "${__Random(210,230)}",
        "current": "${__Random(1,20)}",
        "powerFactor": "${__Random(70,100)}"
    }
}