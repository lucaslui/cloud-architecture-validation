{
    "deviceId": "RES-HOME-${HOME_NUMBER}",
    "deviceType": "RES",
    "eventType": "State",
    "schemaVersion": "1.0",
    "timestamp": "${__time(yyyy-MM-dd'T'HH:mm:ss'Z')}",
    "payload": {
        "gridConnected": "${__chooseRandom(On,Off)}",
        "inverterStatus": "${__chooseRandom(Active,Standby,Fault)}",
